                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        PRODINV.MSOCODE, Sales.ReferenceNo, Sales.Date, PRODTYPELIB.TYPEDESC AS ProductType, PRODINV.CODE AS ProductCode, 
                         PRODFLE.PRODDESC AS ProductDescription, Sales.Quantity, Sales.PesoEquiv
FROM            PRODINV LEFT OUTER JOIN
                         PRODTYPELIB ON PRODINV.TYPE = PRODTYPELIB.TYPE LEFT OUTER JOIN
                         PRODFLE ON PRODFLE.TYPE = PRODINV.TYPE AND PRODFLE.CODE = PRODINV.CODE LEFT OUTER JOIN
                         PRODPRICE ON PRODPRICE.TYPE = PRODINV.TYPE AND PRODPRICE.CODE = PRODINV.CODE LEFT OUTER JOIN
                             (SELECT        ORDERFL.MSOCODE, ORDERDT.ORDERNO AS ReferenceNo, CONVERT(varchar(10), ORDERFL.PAIDDATE, 101) AS Date, ORDERDT.TYPE, 
                                                         ORDERDT.PRICE * ORDERDT.QTY AS PesoEquiv, ORDERDT.CODE, ORDERDT.QTY AS Quantity
                               FROM            ORDERDT INNER JOIN
                                                         ORDERFL ON ORDERFL.ORDERNO = ORDERDT.ORDERNO
                               WHERE        (ORDERFL.MSOCODE = @MSOCODE) AND (ORDERFL.ORDERDATE BETWEEN @STARTDATE AND @ENDDATE)) AS Sales ON 
                         PRODINV.MSOCODE = Sales.MSOCODE AND PRODINV.TYPE = Sales.TYPE AND PRODINV.CODE = Sales.CODE
WHERE        (PRODINV.MSOCODE = @MSOCODE) AND (Sales.ReferenceNo IS NOT NULL)
GROUP BY PRODINV.MSOCODE, PRODTYPELIB.TYPEDESC, PRODFLE.PRODDESC, PRODINV.CODE, Sales.ReferenceNo, Sales.Date, Sales.Quantity, Sales.PesoEquiv
ORDER BY Sales.Quantity DESC</CommandText>
