        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ReOrderTableAdapter" GeneratorDataComponentClassName="ReOrderTableAdapter" Name="ReOrder" UserDataComponentName="ReOrderTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="NPASISConnectionString (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT PRODTYPELIB.TYPEDESC,PRODFLE.CODE ,PRODDESC AS [Title] ,PRODFLE.SCALE,(INV.Inventory-Coalesce([ORDER].Sold,0)) as [OnHand],PRODFLE.Reorder 
FROM PRODFLE 


LEFT JOIN PRODTYPELIB ON PRODFLE.TYPE = PRODTYPELIB.TYPE


RIGHT OUTER JOIN (SELECT MSOCODE, CODE,[TYPE],SUM([Count]) as Inventory FROM PRODINV WHERE 
					MSOCODE=@MSOCODE  GROUP BY MSOCODE,[TYPE],CODE) INV 
					ON PRODFLE.TYPE= INV.TYPE AND PRODFLE.CODE= INV.CODE 

LEFT JOIN (SELECT ORDERDT.MSOCODE,ORDERDT.[TYPE],[CODE],SUM(QTY) as SOLD     
  FROM [NPASIS].[dbo].[ORDERDT]
  INNER JOIN ORDERFL 
  ON ORDERDT.MSOCODE = ORDERFL.MSOCODE AND ORDERDT.ORDERNO = ORDERFL.ORDERNO
  WHERE ORDERDT.MSOCODE=@MSOCODE
  GROUP BY ORDERDT.MSOCODE,[TYPE],CODE
  ) [ORDER] ON PRODFLE.TYPE= [ORDER].TYPE AND PRODFLE.CODE= [ORDER].CODE

WHERE REORDER&gt;(INV.Inventory-Coalesce([ORDER].Sold,0))
ORDER BY PRODFLE.CODE</CommandText>
