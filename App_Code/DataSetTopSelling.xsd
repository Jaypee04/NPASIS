                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TOP(@TOP) PRODTYPELIB.TYPEDESC AS [ProductType],ORDERDT.[CODE] AS Code,PRODFLE.PRODDESC AS [Description],SUM(QTY)as Quantity,SUM(PRICE)as [Peso]
  FROM [NPASIS].[dbo].[ORDERDT]
  
  INNER JOIN ORDERFL
  
  ON ORDERFL.MSOCODE=ORDERDT.MSOCODE AND ORDERFL.ORDERNO=ORDERDT.ORDERNO AND (ORDERDATE BETWEEN @STARTDATE AND @ENDDATE)
  
  LEFT JOIN PRODFLE ON ORDERDT.TYPE = PRODFLE.TYPE  AND  ORDERDT.CODE = PRODFLE.CODE 
  LEFT JOIN PRODTYPELIB ON ORDERDT.TYPE = PRODTYPELIB.TYPE  
  
  
 GROUP BY ORDERDT.[TYPE],ORDERDT.[CODE],PRODFLE.PRODDESC,PRODTYPELIB.TYPEDESC
 
 ORDER BY  QUantity DESC</CommandText>
