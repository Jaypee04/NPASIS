                      <Parameter AllowDbNull="true" AutogeneratedName="STARTDATE" ColumnName="ORDERDATE" DataSourceName="NPASIS.dbo.ORDERFL" DataTypeServer="datetime" DbType="DateTime" Direction="Input" ParameterName="@STARTDATE" Precision="0" ProviderType="DateTime" Scale="0" Size="8" SourceColumn="ORDERDATE" SourceColumnNullMapping="false" SourceVersion="Current" />
